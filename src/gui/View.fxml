   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="50.0" layoutY="125.0" onAction="#onComboBoxAction" prefWidth="150.0" promptText="Select person..." />
      <Button fx:id="btall" layoutX="245.0" layoutY="125.0" mnemonicParsing="false" onAction="#onBtAllAction" prefHeight="25.0" prefWidth="50.0" text="All" />
   </children>
</AnchorPane>
